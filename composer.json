{
    "name": "dosamigos/yii2-semantic-ui",
    "description": "Semantic UI Extension library for Yii2",
    "keywords": [
        "dosamigos",
        "yii",
        "extension",
        "library",
        "yii2-semantic-ui"
    ],
    "homepage": "https://github.com/dosamigos/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "yiisoft/yii2" : "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dosamigos\\semantic\\test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
