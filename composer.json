{
    "name": "2amigos/yii2-semantic-ui",
    "description": "Semantic UI Extension library for Yii2",
    "keywords": [
        "dosamigos",
        "yii",
        "extension",
        "library",
        "yii2-semantic-ui"
    ],
    "homepage": "https://github.com/2amigos/yii2-semantic-ui",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.2",
        "bower-asset/semantic-ui": "1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\semantic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dosamigos\\semantic\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
