{
    "name": "2amigos/yii2-semantic-ui",
    "description": "Semantic UI Extension library for Yii2",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "library",
        "yii2-semantic-ui"
    ],
    "homepage": "http://yiiwheels.com/extension/semantic-ui-library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/yii2-semantic-ui/issues",
        "source": "https://github.com/2amigos/yii2-semantic-ui"
    },
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/semantic-ui": "1.11.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\semantic\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
